{
  "extends" : "airbnb",
  "parserOptions" : {
    "ecmaVersion" : 8,
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "env" : {
    "node": true,
    "browser" : true,
    "es6" : true,
    "jest" : true,
    "jquery" : true
  },

  "rules" :{
    "comma-dangle":["error","never"],
    "linebreak-style":"off",
    "no-unused-vars": ["error", {
       "vars" : "local", 
       "args" : "none"
    }],

    "react/jsx-filename-extension" : "off"
  }
}